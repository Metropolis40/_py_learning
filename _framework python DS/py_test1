# Visualization with Matplotlib
import matplotlib as mpl
import matplotlib.pyplot as plt



plt.style.use('classic')


# %%
import numpy as np


x = np.linspace(0, 10, 100)

plt.plot(x, np.sin(x))

plt.show()

